
<!doctype html>
<html {{ site.language_attributes }}>
<head>
    <meta charset="{{ site.charset }}" />
    <meta name="description" content="{{ site.description }}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="author" href="{{ site.theme.link }}/humans.txt" />
    <link rel="pingback" href="{{ site.pingback_url }}" />
    <link rel="profile" href="http://gmpg.org/xfn/11">
    {{function('wp_head')}}
</head>
<body class="font-body font-light bg-white antialiased">
<div id="site-heading"><div is="kc-nav" inline-template>
<nav class="bg-white bg-opacity-95 w-full site-navigation flex items-center justify-between md:justify-start flex-wrap" role="navigation">
  <div class="flex items-center text-white p-6 md:px-10 md:mr-4">
    {% include "svg/logo.svg" %}
  </div>

  <div class="block md:hidden p-4">
    <button aria-label="Open menu" v-on:click="toggleNav()" class="flex items-center px-3 py-2 border rounded md:hidden">
      {% include "svg/menu-open.svg" %}
    </button>
  </div>

  <div v-bind:class="{ hidden: !isNavShown }" v-on:click.self="toggleNav()" class="w-full h-screen bg-black bg-opacity-50 fixed z-10 left-0 top-0 md:bg-transparent md:p-0 md:h-auto md:static md:flex md:flex-row md:items-center md:w-auto">
    <div class="w-2/3 fixed top-0 right-0 bottom-0 bg-white p-10 md:p-0 md:static md:w-auto">
    {% include "menu.twig" with {'items': menu.get_items} %}
    </div>
  </div>
</nav>
</div>
</div>
{% block content %}
No content
{% endblock %}

{% include "footer.twig" with {'items': menufooter.get_items} %}

{{ function('wp_footer') }}
</body>
</html>