{% extends "contentlisting.twig" %}

{% block content %}
<div class="flex flex-wrap justify-around w-full">
    {% for post in posts %}
        <div class="w-40 p-2"> 
        {% include ['listview-' ~ post.post_type ~ '.twig', 'listview.twig'] %}
        </div>
    {% endfor %}
</div>
{% endblock %}